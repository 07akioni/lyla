{
  "name": "@lylajs/core",
  "version": "0.4.9",
  "keywords": [
    "http-request",
    "http-client",
    "request",
    "xhr",
    "xmlhttprequest"
  ],
  "module": "es/index.js",
  "main": "lib/index.js",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "check-type": "tsc -p tsconfig.cjs.json --noEmit",
    "build": "rm -rf es lib && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "release": "pnpm run build-and-test && pnpm publish",
    "dev": "vite",
    "test:type": "tsc --noEmit",
    "test": "pnpm run test:type && ./tests/before-test.sh && playwright test && ./tests/after-test.sh",
    "build-and-test": "pnpm run test:type && ./tests/before-test.sh && pnpm run build && playwright test && ./tests/after-test.sh",
    "prepublishOnly": "pnpm run build"
  },
  "author": "07akioni",
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@playwright/test": "^1.20.1",
    "axios": "^0.26.1",
    "express": "^4.17.3",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "@types/express": "^4.17.13"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
