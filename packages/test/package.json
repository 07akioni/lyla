    "@rollup/plugin-node-resolve": "^13.2.0",
    "@types/express": "^4.17.13",
    "axios": "^0.27.2",
    "express": "^4.17.3",
    "rollup": "^2.70.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^1.0.4"
  }
}
